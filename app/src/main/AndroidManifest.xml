<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.application">

    <uses-permission android:name="android.permission.INTERNET"/>
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:usesCleartextTraffic="true"
        android:theme="@style/Theme.Application">
        <activity android:name=".vue.EtudiantActivity"/>
        <activity android:name=".vue.NiveauActivity"/>
        <activity android:name=".vue.MatiereActivity"/>
        <activity android:name=".vue.NoteActivity"/>
        <activity android:name=".vue.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".vue.NewEtudiantActivity"/>
        <activity android:name=".vue.NewNiveauActivity"/>
        <activity android:name=".vue.NewMatiereActivity"/>
        <activity android:name=".vue.NewNoteActivity"/>
        <activity android:name=".vue.BulletinActivity"/>
        <activity android:name=".vue.ClassementActivity"/>
        <activity android:name=".vue.EtudiantClickActivity"/>
        <activity android:name=".vue.MatiereClickActivity"/>
        <activity android:name=".vue.NoteClickActivity"/>
    </application>

</manifest>